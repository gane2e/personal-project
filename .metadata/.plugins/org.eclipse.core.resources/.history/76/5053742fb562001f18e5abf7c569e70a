//두 정수 A와 B를 입력받은 다음, A+B를 출력하는 프로그램을 작성하시오.

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		

		int a,b;
		a=0;
		b=0;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("a, b를 입력하세요.");
		int a,b = sc.nextInt();
		
		
		int sum = 0;
		sum = a+b;
		
		System.out.println("합계 : " + a+b);
		
		

	}

}
